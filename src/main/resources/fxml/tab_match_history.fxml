<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.izis.work.MatchHistoryController">
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tab_matchName" prefWidth="75.0" text="比赛名称" />
          <TableColumn fx:id="tab_sponsor" prefWidth="75.0" text="举办单位" />
            <TableColumn fx:id="tab_referee" prefWidth="75.0" text="裁判长" />
            <TableColumn fx:id="tab_arrange" prefWidth="75.0" text="编排长" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
